<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="AL">
        <createTable tableName="AL_AUDIT_LOG">
            <column name="ENTRY_NO" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_AL_AUDIT_LOG"/>
            </column>
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="TABLE_CAPTION" type="VARCHAR(255)"/>
            <column name="TABLE_NO" type="INT"/>
            <column name="DOCUMENT_TYPE" type="VARCHAR(255)"/>
            <column name="DOCUMENT_NO" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="VARCHAR(255)"/>
            <column name="FIELD_CAPTION" type="VARCHAR(255)"/>
            <column name="TYPE_OF_CHANGE" type="VARCHAR(255)"/>
            <column name="OLD_VALUE" type="VARCHAR(255)"/>
            <column name="NEW_VALUE" type="VARCHAR(255)"/>
            <column name="REFERENCE_DESCRIPTION" type="VARCHAR(255)"/>
            <column name="PRIMARY_KEY_FIELD1_DESCRIPTION" type="VARCHAR(255)"/>
            <column name="PRIMARY_KEY_FIELD1_VALUE" type="VARCHAR(255)"/>
            <column name="PRIMARY_KEY_FIELD2_DESCRIPTION" type="VARCHAR(255)"/>
            <column name="PRIMARY_KEY_FIELD2_VALUE" type="VARCHAR(255)"/>
            <column name="PRIMARY_KEY_FIELD3_DESCRIPTION" type="VARCHAR(255)"/>
            <column name="PRIMARY_KEY_FIELD3_VALUE" type="VARCHAR(255)"/>
            <column name="PRIMARY_KEY" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>